{
  "name": "rnny",
  "version": "0.0.1",
  "description": "Repository of my new website",
  "author": "ronnyrook",
  "repository": {
    "type": "git",
    "url": "https://github.com/rnnyrk/rnny.nl"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.js",
    "build": "rimraf dist/ && webpack -p --hide-modules --config config/webpack.config.prod.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .jsx --ext .js src/",
    "analyzer:build": "NODE_ENV=production webpack -p --config webpack.config.prod.js --profile --json > stats.json",
    "analyzer:run": "webpack-bundle-analyzer stats.json ./dist",
    "analyzer": "run-s analyzer:build analyzer:run"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "compression": "^1.7.3",
    "express": "4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "1.10.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "1.0.1",
    "eslint": "5.10.0",
    "eslint-config-react-app": "3.0.5",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "4.1.5",
